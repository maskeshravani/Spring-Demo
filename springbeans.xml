<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

<bean id = "std" class = "com.student.Student">

<property name = "id" value = "5"></property>
<property name = "name" value = "Shravani"></property>
<property name = "add" value = "Kolhapur"></property> 
<property name="con" ref="c"></property> 


<!-- 
<constructor-arg index = "0" value = "3"></constructor-arg>
<constructor-arg index = "1" value = "Shravani"></constructor-arg>
<constructor-arg index = "2" value = "Pune"></constructor-arg>
 -->

</bean>
<bean id = "c" class = "com.student.Country">
<property name="c_id" value = "+91"></property>
<property name="c_name" value = "India"></property>

</bean>

</beans>